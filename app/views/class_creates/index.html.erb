<h1>Class List</h1>
<table id="list">
	<tr>
		<th>Name</th>
		<th colspan="3"></th>
	</tr>
	<% @createClasses.each do |cClasses| %>
		<tr>
			<td> <%= cClasses.class_name %> </td>
			<td><%= link_to 'Details', cClasses %></td>
        	<td><%= link_to 'Edit', edit_class_create_path(cClasses) %></td>
        	<td><%= link_to 'Destroy', cClasses, method: :delete,
                    data: { confirm: 'Are you sure?' } %></td>

		</tr>
	<% end %>
	
</table>