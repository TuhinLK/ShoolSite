<p>
	<strong>Class Name:</strong>
	<%= @classCreate.class_name %>
</p>
<p>
	<strong>School Name:</strong>
	<%= @classCreate.school.school_name %>
</p>
<h3>Student List of <%= @classCreate.class_name %></h3>
<% @classCreate.students.each do |student| %>
	<p>
		<strong>Name</strong>
		<%= student.fullname %>
	</p>
<% end %>


<%= link_to 'Edit', edit_class_create_path(@classCreate) %>
<%= link_to 'Back', class_creates_path %>
